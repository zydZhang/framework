{
	"name": "shadon/framework",
	"description": "shadon php framework",
	"keywords": [
		"api",
		"phalcon",
		"modules"
	],
	"type": "library",
	"license": "MIT",
	"authors": [
		{
			"name": "Shadon",
			"homepage": "http://www.shadon.org"
		}
	],
	"require": {
		"php": "^7.1.3",
		"guzzlehttp/guzzle": "^6.2.3",
		"mongodb/mongodb": "^1.1.2",
		"monolog/monolog": "^1.22.1",
		"php-amqplib/thumper": "^0.5.0",
		"predis/predis": "^1.1.1",
		"ralouphie/getallheaders": "^2.0.5",
		"swiftmailer/swiftmailer": "^6.0.1",
		"vlucas/phpdotenv": "^2.4"
	},
	"require-dev": {
		"techpivot/phalcon-ci-installer": "~1.0"
	},
	"autoload": {
		"psr-4": {
			"Shadon\\": "src"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "1.0-dev"
		}
	},
	"config": {
		"sort-packages": true
	}
}
